<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".ihz.IhzActivity">

    <Button
        android:id="@+id/btn_code_not_200"
        style="@style/btn"
        android:layout_marginTop="200pt"
        android:text="访问服务器成功但是code不等于200"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_data_null"
        style="@style/btn"
        android:layout_marginTop="50pt"
        android:text="code等于200但是data为空"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_code_not_200" />
</androidx.constraintlayout.widget.ConstraintLayout>